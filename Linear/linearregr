import numpy as np
from sklearn.linear_model import LinearRegression
import matplotlib.pyplot as plt
from matplotlib import style

style.use('fivethirtyeight')

xs = [1,2,3,4,5]
ys = [3,5,7,8,9]

clf = LinearRegression()
y_test=clf.fit(xs,ys).predict(11)

print(y_test)